{
  "name": "layton-watchface",
  "author": "robinfire110",
  "version": "1.3.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "pebble": {
    "displayName": "Professor Layton - Mini London",
    "uuid": "e0eeae9d-0fb0-4160-95d1-72cad065d85f",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "basalt",
      "chalk",
      "emery"
    ],
    "watchapp": {
      "watchface": true
    },
    "capabilities": [
      "configurable"
    ],
    "messageKeys": [
      "VibrateOnDisconnect",
      "HourMode",
      "DateFormat",
      "Character1",
      "Character2",
      "Character3",
      "Character1Spoiler",
      "Character2Spoiler",
      "Character3Spoiler",
      "SpoilerMode",
      "RandomInterval",
      "RandomIntervalUnit",
      "ShuffleOptions",
      "CustomTime"
    ],
    "resources": {
      "media": [
        {
          "characterRegex": "[:0-9]",
          "type": "font",
          "name": "FONT_LAYTON_80",
          "file": "fonts/layton_font.ttf",
          "compatibility": "2.7"
        },
        {
          "characterRegex": "[:0-9]",
